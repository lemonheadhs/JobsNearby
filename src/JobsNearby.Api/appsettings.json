{
    "baidu_map_app_key": "ak",
    "deployed_sites": "http://localhost:5000",
    "storage_conn_str": "UseDevelopmentStorage=true",
    "Serilog": {
        "Using": ["Serilog.Sinks.Console"],
        "MinimumLevel": {
            "Default": "Warning",
            "Override": {
                "Microsoft": "Information"
            }
        },
        "WriteTo": [
            { "Name": "Console" }
        ],
        "Enrich": ["FromLogContext", "WithMachineName"]
    }
}